			<class name="com.accesscontrol.projectadmin.CardStandard" />
			<class name="com.accesscontrol.projectadmin.Card" />
			<class name="com.accesscontrol.projectadmin.CardReader" />
			<class name="com.accesscontrol.projectadmin.FaceReader" />
			<class name="com.accesscontrol.projectadmin.Controller" />
